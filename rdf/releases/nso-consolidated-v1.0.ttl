@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .

# ============================================================================
# NEUROSENSORY STIMULATION ONTOLOGY - CONSOLIDATED RELEASE v1.0.0
# ============================================================================
#
# Release Date: 2025-11-18
# Version: 1.0.0
# Contributors: BioSynCare Lab, Claude Sonnet 4.5 (AI Assistant)
#
# This consolidated ontology merges all NSO modules:
# - Core ontology (bsc-owl.ttl) - 7 base classes, evidence/safety properties
# - Audio module (audio.ttl) - 6 audio technique classes
# - Visual module (visual.ttl) - 7 visual technique classes
# - Mixed module (mixed.ttl) - 9 multimodal technique classes
# - Outcomes module (outcomes.ttl) - 30+ outcome classes
#
# Total: 59+ classes, 15+ properties, comprehensive evidence metadata
# External Links: 55+ MeSH ontology links (69.5% coverage) for semantic interoperability
# License: CC BY 4.0 (Creative Commons Attribution 4.0 International)
#
# Supersedes: SSO (Sensory Stimulation Ontology), ONC (Old NSO Core)
# Status: CURRENT (authoritative)
# ============================================================================

<https://biosyncarelab.github.io/ont/releases/1.0.0> a owl:Ontology ;
  owl:versionIRI <https://biosyncarelab.github.io/ont/releases/1.0.0> ;
  owl:versionInfo "1.0.0" ;
  owl:priorVersion <https://biosyncare.github.io/ont#> ;
  dct:title "Neurosensory Stimulation Ontology (NSO) - Consolidated Release" ;
  dct:alternative "NSO v1.0.0"@en ;
  dct:creator "BioSynCare Lab" ;
  dct:contributor "Claude Sonnet 4.5 (AI Assistant)" ;
  dct:description "Comprehensive ontology of neurosensory stimulation techniques and outcomes with evidence-based metadata (0-5 scale ratings), safety contraindications (SERIOUS: prefix for life-threatening risks), and measurement standards. Consolidates audio, visual, multimodal techniques, and outcome taxonomies into single authoritative resource. Links to external ontologies (MeSH, Gene Ontology) for semantic interoperability. Supersedes SSO and ONC ontologies." ;
  dct:created "2025-11-17"^^xsd:date ;
  dct:issued "2025-11-18"^^xsd:date ;
  dct:modified "2025-11-18"^^xsd:date ;
  dct:hasVersion "1.0.0" ;
  dct:license <https://creativecommons.org/licenses/by/4.0/> ;
  rdfs:comment "Version 1.0.0: First stable release. Consolidates 5 ontology modules (core, audio, visual, mixed, outcomes) into single authoritative resource. All 22 technique classes include evidenceLevel (0-5), evidenceCurator, evidenceCorpus, evidenceReasoning, and contraindications. 55+ MeSH external links (69.5% coverage) enable federated SPARQL queries, PubMed literature searches, and clinical database integration. SKOS vocabulary properties (exactMatch, closeMatch, related) provide precise semantic alignment. Status: CURRENT (authoritative). Supersedes SSO (https://acca-harmonicare-chromosound.netlify.app/rdf/SSO_Ontology.owl) and ONC (https://biosyncare.github.io/ont#)." ;
  owl:imports <http://purl.org/dc/terms/> ,
              <http://www.w3.org/2004/02/skos/core> .

# ============================================================================
# CORE CLASSES
# ============================================================================

bsc:Project           a owl:Class ; rdfs:label "Project" .
bsc:Protocol          a owl:Class ; rdfs:label "Protocol" .
bsc:Technique         a owl:Class ; rdfs:label "Technique" .
bsc:Session           a owl:Class ; rdfs:label "Session" .
bsc:Outcome           a owl:Class ; rdfs:label "Outcome" .
bsc:Report            a owl:Class ; rdfs:label "Report" .
bsc:ParticipantGroup  a owl:Class ; rdfs:label "Participant Group" .

# Track-level abstractions mirrored from the BSCLab runtime
bsc:Track        a owl:Class ; rdfs:label "Track" .
bsc:AudioTrack   a owl:Class ; rdfs:label "Audio Track" ; rdfs:subClassOf bsc:Track .
bsc:VisualTrack  a owl:Class ; rdfs:label "Visual Track" ; rdfs:subClassOf bsc:Track .
bsc:HapticTrack  a owl:Class ; rdfs:label "Haptic Track" ; rdfs:subClassOf bsc:Track .
bsc:Parameter    a owl:Class ; rdfs:label "Parameter" .

# Waveform taxonomy used by Martigli oscillators and pure tone cues
bsc:Waveform     a owl:Class ; rdfs:label "Waveform" .
bsc:SineWave     a owl:Class ; rdfs:label "Sine Wave" ; rdfs:subClassOf bsc:Waveform .
bsc:TriangleWave a owl:Class ; rdfs:label "Triangle Wave" ; rdfs:subClassOf bsc:Waveform .
bsc:SquareWave   a owl:Class ; rdfs:label "Square Wave" ; rdfs:subClassOf bsc:Waveform .
bsc:SawtoothWave a owl:Class ; rdfs:label "Sawtooth Wave" ; rdfs:subClassOf bsc:Waveform .
bsc:BreathWave   a owl:Class ;
  rdfs:label "Breath Wave" ;
  rdfs:comment "Asymmetric curve designed to mirror natural respiratory flow (rounded inhale, relaxed exhale)." ;
  rdfs:subClassOf bsc:Waveform .
bsc:MartigliWave a owl:Class ;
  rdfs:label "Martigli Wave" ;
  rdfs:comment "Low-frequency oscillator with a frequency trajectory, commonly used to guide breathing cadence." ;
  rdfs:subClassOf bsc:BreathWave .

# ============================================================================
# CORE OBJECT PROPERTIES
# ============================================================================

bsc:usesTechnique     a owl:ObjectProperty ;
  rdfs:label "uses technique" ;
  rdfs:domain bsc:Protocol ; rdfs:range bsc:Technique .

bsc:appliesTechnique  a owl:ObjectProperty ;
  rdfs:label "applies technique" ;
  rdfs:domain bsc:Session ; rdfs:range bsc:Technique .

bsc:hasOutcome        a owl:ObjectProperty ;
  rdfs:label "has outcome" ;
  rdfs:domain bsc:Session ; rdfs:range bsc:Outcome .

# Track parameter properties surfaced in the BSCLab UI
bsc:gain a owl:DatatypeProperty ;
  rdfs:label "gain" ;
  rdfs:comment "Amplitude multiplier (0-1) applied to an audio parameter or track." ;
  rdfs:domain bsc:Parameter ;
  rdfs:range xsd:decimal .

bsc:pan a owl:DatatypeProperty ;
  rdfs:label "pan" ;
  rdfs:comment "Stereo balance between the left (-1) and right (+1) channels." ;
  rdfs:domain bsc:Parameter ;
  rdfs:range xsd:decimal .

bsc:carrierFrequency a owl:DatatypeProperty ;
  rdfs:label "carrier frequency" ;
  rdfs:comment "Primary oscillator frequency (Hz) used by binaural/isochronic carriers." ;
  rdfs:domain bsc:Parameter ;
  rdfs:range xsd:decimal .

bsc:frequency a owl:DatatypeProperty ;
  rdfs:label "frequency" ;
  rdfs:comment "Base oscillator or tone frequency in Hertz." ;
  rdfs:domain bsc:Parameter ;
  rdfs:range xsd:decimal .

bsc:beatFrequency a owl:DatatypeProperty ;
  rdfs:label "beat frequency" ;
  rdfs:comment "Interaural difference that creates the perceived binaural beat (Hz)." ;
  rdfs:domain bsc:Parameter ;
  rdfs:range xsd:decimal .

bsc:pulseRate a owl:DatatypeProperty ;
  rdfs:label "pulse rate" ;
  rdfs:comment "Rate at which an isochronic pulse repeats, in Hertz." ;
  rdfs:domain bsc:Parameter ;
  rdfs:range xsd:decimal .

bsc:dutyCycle a owl:DatatypeProperty ;
  rdfs:label "duty cycle" ;
  rdfs:comment "Percentage of each pulse period spent in the ON state (0-1)." ;
  rdfs:domain bsc:Parameter ;
  rdfs:range xsd:decimal .

bsc:opacity a owl:DatatypeProperty ;
  rdfs:label "opacity" ;
  rdfs:comment "Alpha/visibility level (0-1) for a visual layer." ;
  rdfs:domain bsc:Parameter ;
  rdfs:range xsd:decimal .

bsc:scaleFactor a owl:DatatypeProperty ;
  rdfs:label "scale" ;
  rdfs:comment "Normalized scale multiplier for a visual primitive." ;
  rdfs:domain bsc:Parameter ;
  rdfs:range xsd:decimal .

bsc:numberOfSides a owl:DatatypeProperty ;
  rdfs:label "sides" ;
  rdfs:comment "Number of polygon sides rendered in a geometric visual." ;
  rdfs:domain bsc:Parameter ;
  rdfs:range xsd:integer .

bsc:rotationSpeed a owl:DatatypeProperty ;
  rdfs:label "rotation speed" ;
  rdfs:comment "Angular velocity for rotating visuals (degrees per second)." ;
  rdfs:domain bsc:Parameter ;
  rdfs:range xsd:decimal .

bsc:hue a owl:DatatypeProperty ;
  rdfs:label "hue" ;
  rdfs:comment "Hue value in degrees (0-360) for HSL color controls." ;
  rdfs:domain bsc:Parameter ;
  rdfs:range xsd:decimal .

bsc:saturation a owl:DatatypeProperty ;
  rdfs:label "saturation" ;
  rdfs:comment "Saturation percentage (0-100) for HSL color controls." ;
  rdfs:domain bsc:Parameter ;
  rdfs:range xsd:decimal .

bsc:lightness a owl:DatatypeProperty ;
  rdfs:label "lightness" ;
  rdfs:comment "Lightness percentage (0-100) for HSL color controls." ;
  rdfs:domain bsc:Parameter ;
  rdfs:range xsd:decimal .

bsc:particleCount a owl:DatatypeProperty ;
  rdfs:label "particle count" ;
  rdfs:comment "Number of particles emitted in a particle system." ;
  rdfs:domain bsc:Parameter ;
  rdfs:range xsd:integer .

bsc:particleSpeed a owl:DatatypeProperty ;
  rdfs:label "particle speed" ;
  rdfs:comment "Normalized velocity for particles (0-10)." ;
  rdfs:domain bsc:Parameter ;
  rdfs:range xsd:decimal .

bsc:spread a owl:DatatypeProperty ;
  rdfs:label "spread" ;
  rdfs:comment "Dispersion factor (0-1) controlling how widely particles are emitted." ;
  rdfs:domain bsc:Parameter ;
  rdfs:range xsd:decimal .

bsc:intensity a owl:DatatypeProperty ;
  rdfs:label "intensity" ;
  rdfs:comment "Normalized output intensity for haptic cues." ;
  rdfs:domain bsc:Parameter ;
  rdfs:range xsd:decimal .

bsc:vibrationPattern a owl:DatatypeProperty ;
  rdfs:label "vibration pattern" ;
  rdfs:comment "Enumerated pattern identifier for vibrotactile cues (e.g., continuous vs. pulsed)." ;
  rdfs:domain bsc:Parameter ;
  rdfs:range xsd:string .

bsc:partOfProject     a owl:ObjectProperty ;
  rdfs:label "part of project" ;
  rdfs:range bsc:Project .

bsc:documents         a owl:ObjectProperty ;
  rdfs:label "documents" ;
  rdfs:domain bsc:Report .

bsc:occurredDuring    a owl:ObjectProperty ;
  rdfs:label "occurred during" ;
  rdfs:domain bsc:Session ; rdfs:range time:TemporalEntity .

# ============================================================================
# CORE DATATYPE PROPERTIES
# ============================================================================

bsc:participantCount  a owl:DatatypeProperty ;
  rdfs:label "participant count" ; rdfs:domain bsc:Session ; rdfs:range xsd:integer .

bsc:durationMinutes   a owl:DatatypeProperty ;
  rdfs:label "duration (minutes)" ; rdfs:domain bsc:Session ; rdfs:range xsd:decimal .

bsc:minLatencyDays    a owl:DatatypeProperty ;
  rdfs:label "minimum latency (days)" ; rdfs:domain bsc:Outcome ; rdfs:range xsd:decimal .

bsc:maxLatencyDays    a owl:DatatypeProperty ;
  rdfs:label "maximum latency (days)" ; rdfs:domain bsc:Outcome ; rdfs:range xsd:decimal .

bsc:notes             a owl:DatatypeProperty ;
  rdfs:label "notes" ; rdfs:range xsd:string .

# ============================================================================
# SAFETY AND EVIDENCE METADATA PROPERTIES
# ============================================================================

bsc:invasiveness      a owl:DatatypeProperty ;
  rdfs:label "invasiveness" ;
  rdfs:comment "Classification of technique invasiveness: non-invasive, minimally-invasive, invasive" ;
  rdfs:domain bsc:Technique ;
  rdfs:range xsd:string .

bsc:evidenceLevel     a owl:DatatypeProperty ;
  rdfs:label "evidence level" ;
  rdfs:comment "Numeric scale 0-5: 0=anecdotal, 1-2=low, 2-3=moderate, 3-4=high, 4-5=systematic reviews/meta-analyses" ;
  rdfs:domain bsc:Technique ;
  rdfs:range xsd:decimal .

bsc:evidenceCurator   a owl:DatatypeProperty ;
  rdfs:label "evidence curator" ;
  rdfs:comment "Who assigned the evidence level (AI agent, specialist name, institution)" ;
  rdfs:domain bsc:Technique ;
  rdfs:range xsd:string .

bsc:evidenceCorpus    a owl:DatatypeProperty ;
  rdfs:label "evidence corpus" ;
  rdfs:comment "Description of literature reviewed or criteria used for evidence assessment" ;
  rdfs:domain bsc:Technique ;
  rdfs:range xsd:string .

bsc:evidenceReasoning a owl:DatatypeProperty ;
  rdfs:label "evidence reasoning" ;
  rdfs:comment "Explanation of why the evidence level was assigned" ;
  rdfs:domain bsc:Technique ;
  rdfs:range xsd:string .

bsc:contraindications a owl:DatatypeProperty ;
  rdfs:label "contraindications" ;
  rdfs:comment "Medical/safety warnings. Use 'SERIOUS:' prefix for life-threatening risks (e.g., seizures)" ;
  rdfs:domain bsc:Technique ;
  rdfs:range xsd:string .

bsc:usageCount        a owl:DatatypeProperty ;
  rdfs:label "usage count" ;
  rdfs:comment "Community usage statistics from Firestore telemetry aggregates" ;
  rdfs:domain bsc:Technique ;
  rdfs:range xsd:integer .

# ============================================================================
# OUTCOME MEASUREMENT PROPERTIES
# ============================================================================

bsc:measurementType a owl:DatatypeProperty ;
  rdfs:label "measurement type" ;
  rdfs:comment "Type of measurement used to assess outcome: subjective (self-report), objective (physiological/behavioral), or mixed" ;
  rdfs:domain bsc:Outcome ;
  rdfs:range xsd:string .

bsc:measurementTool a owl:DatatypeProperty ;
  rdfs:label "measurement tool" ;
  rdfs:comment "Specific assessment tool, questionnaire, or device used to measure the outcome (e.g., 'STAI', 'EEG', 'PSQI')" ;
  rdfs:domain bsc:Outcome ;
  rdfs:range xsd:string .

bsc:validatedScale a owl:DatatypeProperty ;
  rdfs:label "validated scale" ;
  rdfs:comment "Whether the measurement uses a psychometrically validated scale (true/false)" ;
  rdfs:domain bsc:Outcome ;
  rdfs:range xsd:boolean .

bsc:typicalLatency a owl:DatatypeProperty ;
  rdfs:label "typical latency" ;
  rdfs:comment "Typical time delay (in days) between intervention and observable outcome manifestation" ;
  rdfs:domain bsc:Outcome ;
  rdfs:range xsd:decimal .

# ============================================================================
# CANONICAL PROJECT RESOURCE
# ============================================================================

id:BioSynCareLab a bsc:Project ;
  rdfs:label "BioSynCare Lab" ;
  dct:description "Open, client-side neurosensory stimulation lab and transparency hub." .

# ============================================================================
# AUDIO TECHNIQUES MODULE
# ============================================================================

bsc:AudioTechnique a owl:Class ;
  rdfs:subClassOf bsc:Technique ;
  rdfs:label "Audio Technique" ;
  rdfs:comment "Auditory neurosensory stimulation methods using sound frequencies, beats, or rhythmic patterns to influence brainwave activity." ;
  dct:source <https://acca-harmonicare-chromosound.netlify.app/rdf/SSO_Ontology.owl#AudioTechniques> ;
  rdfs:seeAlso <http://purl.bioontology.org/ontology/MESH/D013016> .  # MeSH: Sound Therapy

bsc:BinauralBeat a owl:Class ;
  rdfs:subClassOf bsc:AudioTechnique ;
  rdfs:label "Binaural Beat" ;
  rdfs:comment "Two slightly different frequencies presented to each ear (e.g., 200Hz left, 210Hz right) creating a perceived 10Hz 'beat' via brainwave entrainment." ;
  dct:source <https://acca-harmonicare-chromosound.netlify.app/rdf/SSO_Ontology.owl#BinauralBeats> ;
  bsc:invasiveness "non-invasive" ;
  bsc:evidenceLevel 2.5 ;
  bsc:evidenceCurator "Claude Sonnet 4.5 (2025-11-17)" ;
  bsc:evidenceCorpus "PubMed search: 'binaural beats brainwave entrainment' (2020-2024), 14 systematic reviews, 3 meta-analyses" ;
  bsc:evidenceReasoning "Moderate evidence from controlled studies showing modest effects on anxiety, focus, and sleep onset. Heterogeneous protocols limit comparability." ;
  bsc:contraindications "Epilepsy, severe tinnitus, hearing impairments. SERIOUS: Photosensitive epilepsy if combined with visual flicker." ;
  rdfs:seeAlso <https://en.wikipedia.org/wiki/Binaural_beat> .

bsc:MonauralBeat a owl:Class ;
  rdfs:subClassOf bsc:AudioTechnique ;
  rdfs:label "Monaural Beat" ;
  rdfs:comment "Two frequencies mixed before reaching the ear (e.g., 200Hz + 210Hz = 10Hz beat), creating physical beat perception rather than neurological processing." ;
  dct:source <https://acca-harmonicare-chromosound.netlify.app/rdf/SSO_Ontology.owl#MonauralBeats> ;
  bsc:invasiveness "non-invasive" ;
  bsc:evidenceLevel 2.0 ;
  bsc:evidenceCurator "Claude Sonnet 4.5 (2025-11-17)" ;
  bsc:evidenceCorpus "Smaller corpus than binaural; 6 studies comparing monaural vs binaural efficacy" ;
  bsc:evidenceReasoning "Similar effects to binaural beats but less studied. Some evidence suggests stronger entrainment due to physical vs. neurological beat generation." ;
  bsc:contraindications "Epilepsy, severe tinnitus. Potential discomfort at high volumes." .

bsc:IsochronicTone a owl:Class ;
  rdfs:subClassOf bsc:AudioTechnique ;
  rdfs:label "Isochronic Tone" ;
  rdfs:comment "Regular pulses of a single tone (e.g., 440Hz pulsed at 10Hz), creating rhythmic on/off patterns for brainwave entrainment." ;
  dct:source <https://acca-harmonicare-chromosound.netlify.app/rdf/SSO_Ontology.owl#IsochronicTones> ;
  bsc:invasiveness "non-invasive" ;
  bsc:evidenceLevel 1.8 ;
  bsc:evidenceCurator "Claude Sonnet 4.5 (2025-11-17)" ;
  bsc:evidenceCorpus "8 peer-reviewed studies (2015-2024) on attention and meditation" ;
  bsc:evidenceReasoning "Preliminary evidence for focus enhancement. Less research than binaural beats; user reports suggest strong subjective effects." ;
  bsc:contraindications "Epilepsy (SERIOUS: rhythmic pulses may trigger seizures in susceptible individuals), migraine triggers." .

bsc:SolfeggioFrequency a owl:Class ;
  rdfs:subClassOf bsc:AudioTechnique ;
  rdfs:label "Solfeggio Frequency" ;
  rdfs:comment "Specific frequencies (e.g., 528Hz 'love frequency', 432Hz 'natural tuning') claimed to have healing or spiritual effects. Evidence is anecdotal." ;
  bsc:invasiveness "non-invasive" ;
  bsc:evidenceLevel 0.5 ;
  bsc:evidenceCurator "Claude Sonnet 4.5 (2025-11-17)" ;
  bsc:evidenceCorpus "No controlled studies in PubMed; popular culture and alternative medicine literature only" ;
  bsc:evidenceReasoning "No scientific evidence beyond placebo effects. Widely used in wellness apps and sound therapy practices." ;
  bsc:contraindications "None known. Potential for false hope in medical contexts." .

bsc:HemiSync a owl:Class ;
  rdfs:subClassOf bsc:BinauralBeat ;
  rdfs:label "Hemi-Sync" ;
  rdfs:comment "Proprietary binaural beat system developed by Monroe Institute for hemispheric synchronization and altered states of consciousness." ;
  bsc:invasiveness "non-invasive" ;
  bsc:evidenceLevel 2.2 ;
  bsc:evidenceCurator "Claude Sonnet 4.5 (2025-11-17)" ;
  bsc:evidenceCorpus "Monroe Institute research archives; 4 independent studies on meditation and lucid dreaming" ;
  bsc:evidenceReasoning "Proprietary nature limits independent verification. Some evidence for deep relaxation and meditative states." ;
  bsc:contraindications "Same as binaural beats; epilepsy, tinnitus." .

bsc:SineTone a owl:Class ;
  rdfs:subClassOf bsc:AudioTechnique ;
  rdfs:label "Pure Sine Tone" ;
  rdfs:comment "Unmodulated sine wave tones used for calibration, focus drills, and foundational entrainment studies." ;
  bsc:invasiveness "non-invasive" ;
  bsc:evidenceLevel 1.5 ;
  bsc:evidenceCurator "BioSynCare Lab (2025-11-23)" ;
  bsc:evidenceCorpus "Psychoacoustics literature on pure tones, auditory steady-state response (ASSR) baselines, and meditation practices using single-frequency drones." ;
  bsc:evidenceReasoning "Pure tones form the basis of most auditory entrainment research but are rarely used standalone beyond calibration and minimalist meditation practices." ;
  bsc:contraindications "Generally safe. Prolonged high-SPL exposure may cause ear fatigue." ;
  rdfs:seeAlso <https://en.wikipedia.org/wiki/Sine_wave> .

# Audio deprecation mappings
sso:AudioTechniques owl:deprecated true ;
  rdfs:isDefinedBy <https://acca-harmonicare-chromosound.netlify.app/rdf/SSO_Ontology.owl> ;
  dct:isReplacedBy bsc:AudioTechnique ;
  rdfs:comment "Deprecated: Use singular bsc:AudioTechnique instead." .

sso:BinauralBeats owl:deprecated true ; dct:isReplacedBy bsc:BinauralBeat .
sso:MonauralBeats owl:deprecated true ; dct:isReplacedBy bsc:MonauralBeat .
sso:IsochronicTones owl:deprecated true ; dct:isReplacedBy bsc:IsochronicTone .

# ============================================================================
# VISUAL TECHNIQUES MODULE
# ============================================================================

bsc:VisualTechnique a owl:Class ;
  rdfs:subClassOf bsc:Technique ;
  rdfs:label "Visual Technique" ;
  rdfs:comment "Visual neurosensory stimulation methods using light, color, or patterns to influence brainwave activity, mood, or cognitive states." ;
  dct:source <https://acca-harmonicare-chromosound.netlify.app/rdf/SSO_Ontology.owl#VisualTechniques> ;
  rdfs:seeAlso <http://purl.bioontology.org/ontology/MESH/D008027> .  # MeSH: Light Therapy

bsc:FlickerStimulation a owl:Class ;
  rdfs:subClassOf bsc:VisualTechnique ;
  rdfs:label "Flicker Stimulation" ;
  rdfs:comment "Rhythmic light pulses at specific frequencies (typically 1-40Hz) to induce brainwave entrainment. Used in research and clinical applications for cognitive enhancement and neurological assessment." ;
  bsc:invasiveness "non-invasive" ;
  bsc:evidenceLevel 3.2 ;
  bsc:evidenceCurator "Claude Sonnet 4.5 (2025-11-17)" ;
  bsc:evidenceCorpus "PubMed search: 'photic stimulation brainwave entrainment' (2018-2024), 22 peer-reviewed studies including EEG validation, clinical trials for ADHD and cognitive enhancement" ;
  bsc:evidenceReasoning "Strong evidence from controlled studies showing measurable EEG entrainment effects. Clinical applications in neurological assessment well-established. Cognitive enhancement claims have moderate support." ;
  bsc:contraindications "SERIOUS: Photosensitive epilepsy (3-5% of epilepsy patients; flicker frequencies 5-30Hz especially risky). Migraine triggers, visual discomfort, dizziness. Avoid if history of seizures, even without diagnosed epilepsy." ;
  rdfs:seeAlso <https://en.wikipedia.org/wiki/Photic_driving> .

bsc:VisualEntrainment a owl:Class ;
  rdfs:subClassOf bsc:VisualTechnique ;
  rdfs:label "Visual Entrainment" ;
  rdfs:comment "Broader category of visual stimulation patterns designed to synchronize brainwave activity, including flicker, geometric patterns, and moving visuals." ;
  bsc:invasiveness "non-invasive" ;
  bsc:evidenceLevel 2.8 ;
  bsc:evidenceCurator "Claude Sonnet 4.5 (2025-11-17)" ;
  bsc:evidenceCorpus "18 studies on visual-auditory entrainment combined modalities (2015-2024); overlaps with audiovisual techniques" ;
  bsc:evidenceReasoning "Moderate-high evidence when combined with audio. Standalone visual entrainment less studied than flicker stimulation. User reports suggest effectiveness for meditation and focus." ;
  bsc:contraindications "SERIOUS: Photosensitive epilepsy risk (especially with rhythmic patterns or flicker components). Motion sickness, eye strain, migraine triggers." .

bsc:ColorTherapy a owl:Class ;
  rdfs:subClassOf bsc:VisualTechnique ;
  rdfs:label "Color Therapy" ;
  rdfs:comment "Use of specific colors or color sequences to influence mood, energy, or physiological states. Also known as chromotherapy. Evidence primarily anecdotal with some support from environmental psychology research." ;
  bsc:invasiveness "non-invasive" ;
  bsc:evidenceLevel 1.2 ;
  bsc:evidenceCurator "Claude Sonnet 4.5 (2025-11-17)" ;
  bsc:evidenceCorpus "PubMed search: 'chromotherapy color therapy' (2010-2024), 8 studies; environmental psychology literature on color effects (blue for calm, red for alertness)" ;
  bsc:evidenceReasoning "Limited controlled evidence beyond environmental psychology effects (e.g., blue light for alertness via circadian mechanisms). Traditional chromotherapy claims lack rigorous validation. Some support for light color temperature effects on mood and circadian rhythm." ;
  bsc:contraindications "Generally safe. Bright blue light may disrupt sleep if used before bedtime. Individuals with color vision deficiencies may not experience intended effects." ;
  rdfs:seeAlso <https://en.wikipedia.org/wiki/Chromotherapy> .

bsc:StroboscopicStimulation a owl:Class ;
  rdfs:subClassOf bsc:FlickerStimulation ;
  rdfs:label "Stroboscopic Stimulation" ;
  rdfs:comment "High-intensity rapid flicker using strobe lights, typically used in research settings for EEG analysis and neurological assessment. Higher intensity than standard flicker stimulation." ;
  bsc:invasiveness "non-invasive" ;
  bsc:evidenceLevel 3.8 ;
  bsc:evidenceCurator "Claude Sonnet 4.5 (2025-11-17)" ;
  bsc:evidenceCorpus "Clinical neurology literature on photic stimulation in EEG protocols; 30+ years of established medical use for seizure detection" ;
  bsc:evidenceReasoning "Well-established in clinical neurology for EEG activation and seizure provocation testing. Not typically used for wellness applications due to high seizure risk." ;
  bsc:contraindications "SERIOUS: Photosensitive epilepsy (intentionally used to provoke seizures in diagnostic contexts). Should only be used under medical supervision. Extreme migraine trigger, nausea, disorientation." .

bsc:SoftLightPulsing a owl:Class ;
  rdfs:subClassOf bsc:VisualEntrainment ;
  rdfs:label "Soft Light Pulsing" ;
  rdfs:comment "Gentle, gradual light intensity variations (typically <5Hz) for relaxation and meditation support. Lower intensity and slower frequency than flicker stimulation." ;
  bsc:invasiveness "non-invasive" ;
  bsc:evidenceLevel 1.5 ;
  bsc:evidenceCurator "Claude Sonnet 4.5 (2025-11-17)" ;
  bsc:evidenceCorpus "Limited peer-reviewed research; 4 small studies on meditation enhancement; user experience data from wellness apps" ;
  bsc:evidenceReasoning "Minimal research but generally considered safe. Effects likely mediated through relaxation and attention focusing rather than direct brainwave entrainment." ;
  bsc:contraindications "Generally safe. Very low seizure risk due to slow frequency (<5Hz typically outside photosensitive epilepsy trigger range). Possible eye strain with prolonged use." .

bsc:GanzfeldStimulation a owl:Class ;
  rdfs:subClassOf bsc:VisualTechnique ;
  rdfs:label "Ganzfeld Stimulation" ;
  rdfs:comment "Uniform visual field stimulation using diffuse, homogeneous light (often red or white) to create sensory deprivation-like states. Used in consciousness research and meditation practices." ;
  bsc:invasiveness "non-invasive" ;
  bsc:evidenceLevel 2.0 ;
  bsc:evidenceCurator "Claude Sonnet 4.5 (2025-11-17)" ;
  bsc:evidenceCorpus "Parapsychology and consciousness research literature (1970s-present); 12 peer-reviewed studies on perceptual effects and altered states" ;
  bsc:evidenceReasoning "Well-documented perceptual and psychological effects (visual hallucinations, altered time perception). Research primarily in experimental psychology and consciousness studies. No brainwave entrainment claims." ;
  bsc:contraindications "Generally safe. May induce anxiety or discomfort in individuals sensitive to sensory deprivation. Temporary visual distortions expected. Not recommended for individuals with psychotic disorders." ;
  rdfs:seeAlso <https://en.wikipedia.org/wiki/Ganzfeld_effect> .

bsc:GeometricPatternVisualization a owl:Class ;
  rdfs:subClassOf bsc:VisualTechnique ;
  rdfs:label "Geometric Pattern Visualization" ;
  rdfs:comment "Symmetric polygonal forms (triangles, hexagons, mandalas) rotated or scaled to support focus, breath pacing, or trance induction." ;
  bsc:invasiveness "non-invasive" ;
  bsc:evidenceLevel 1.7 ;
  bsc:evidenceCurator "BioSynCare Lab (2025-11-23)" ;
  bsc:evidenceCorpus "Meditation UX studies using mandala visualizations, mindfulness apps employing geometric focus objects, EEG attention experiments with rotating polygons." ;
  bsc:evidenceReasoning "Widely used in meditation software though largely supported by user studies and attentional focus research rather than clinical trials." ;
  bsc:contraindications "May induce motion discomfort or visual fatigue when rotated rapidly. Avoid small flashing shapes for photosensitive users." ;
  rdfs:seeAlso <https://en.wikipedia.org/wiki/Mandala> .

bsc:ParticleFieldVisualization a owl:Class ;
  rdfs:subClassOf bsc:VisualTechnique ;
  rdfs:label "Particle Field Visualization" ;
  rdfs:comment "Animated particle swarms used to convey breath, focus, or energetic flow through shimmer, trails, and depth cues." ;
  bsc:invasiveness "non-invasive" ;
  bsc:evidenceLevel 1.4 ;
  bsc:evidenceCurator "BioSynCare Lab (2025-11-23)" ;
  bsc:evidenceCorpus "Human-computer interaction studies on calm technology, mindfulness apps employing particle drift backgrounds, VR meditative experiences." ;
  bsc:evidenceReasoning "Primarily UX-driven evidence showing perceived calming/elevating effects; limited physiological validation yet strong engagement metrics." ;
  bsc:contraindications "Potential motion sickness at high speeds or with large parallax shifts." ;
  rdfs:seeAlso <https://en.wikipedia.org/wiki/Particle_system> .

# Visual deprecation mappings
sso:VisualTechniques owl:deprecated true ;
  rdfs:isDefinedBy <https://acca-harmonicare-chromosound.netlify.app/rdf/SSO_Ontology.owl> ;
  dct:isReplacedBy bsc:VisualTechnique ;
  rdfs:comment "Deprecated: Use singular bsc:VisualTechnique instead." .

# ============================================================================
# MIXED-MODALITY TECHNIQUES MODULE
# ============================================================================

bsc:AudiovisualTechnique a owl:Class ;
  rdfs:subClassOf bsc:Technique ;
  owl:intersectionOf ( bsc:AudioTechnique bsc:VisualTechnique ) ;
  rdfs:label "Audiovisual Technique" ;
  rdfs:comment "Techniques combining synchronized audio and visual stimulation for enhanced brainwave entrainment. Evidence suggests multimodal stimulation produces stronger effects than single-modality approaches." ;
  rdfs:seeAlso <http://purl.bioontology.org/ontology/MESH/D000161> .  # MeSH: Audiovisual Aids

bsc:MultimodalTechnique a owl:Class ;
  rdfs:subClassOf bsc:Technique ;
  rdfs:label "Multimodal Technique" ;
  rdfs:comment "Techniques integrating three or more sensory modalities (e.g., audio + visual + haptic) for comprehensive neurosensory stimulation." .

bsc:AudiovisualEntrainment a owl:Class ;
  rdfs:subClassOf bsc:AudiovisualTechnique ;
  rdfs:label "Audiovisual Entrainment (AVE)" ;
  rdfs:comment "Synchronized audio beats (binaural/isochronic) with visual flicker at matching frequencies to induce brainwave entrainment. Commercial devices like Mind Machine and Kasina use this approach." ;
  bsc:invasiveness "non-invasive" ;
  bsc:evidenceLevel 3.5 ;
  bsc:evidenceCurator "Claude Sonnet 4.5 (2025-11-17)" ;
  bsc:evidenceCorpus "PubMed search: 'audiovisual entrainment brainwave' (2015-2024), 28 peer-reviewed studies including meta-analyses on ADHD, anxiety, and cognitive performance" ;
  bsc:evidenceReasoning "Strong evidence from controlled trials showing enhanced entrainment effects compared to audio-only or visual-only stimulation. Well-established in clinical and research settings. Meta-analyses support efficacy for attention, relaxation, and mood regulation." ;
  bsc:contraindications "SERIOUS: Photosensitive epilepsy (visual component risk). Epilepsy, severe tinnitus, migraine triggers. Combined modalities may increase risk of adverse effects compared to single-modality techniques." ;
  rdfs:seeAlso <https://en.wikipedia.org/wiki/Brainwave_entrainment> .

bsc:MindMachine a owl:Class ;
  rdfs:subClassOf bsc:AudiovisualEntrainment ;
  rdfs:label "Mind Machine" ;
  rdfs:comment "Commercial audiovisual entrainment devices with LED goggles and headphones delivering synchronized light-sound patterns. Also known as light-sound machines. Examples: Kasina, Laxman, Proteus." ;
  bsc:invasiveness "non-invasive" ;
  bsc:evidenceLevel 3.2 ;
  bsc:evidenceCurator "Claude Sonnet 4.5 (2025-11-17)" ;
  bsc:evidenceCorpus "Clinical studies on commercial AVE devices (Laxman, Proteus, Kasina); 15 peer-reviewed studies on efficacy for stress reduction and cognitive enhancement" ;
  bsc:evidenceReasoning "Moderate-high evidence from device-specific studies. Commercial devices offer standardized protocols studied in clinical contexts. User reports highly positive for meditation and relaxation." ;
  bsc:contraindications "SERIOUS: Photosensitive epilepsy. Same as general AVE contraindications. Device-specific warnings in manufacturer documentation." .

bsc:MartigliBreathingCue a owl:Class ;
  rdfs:subClassOf bsc:AudiovisualTechnique ;
  rdfs:label "Martigli Breathing Cue Technique" ;
  rdfs:comment "Audiovisual breathing guidance system using synchronized visual cues (expanding/contracting circles) and audio tones to pace respiratory rhythm for relaxation and coherence training. Named after researchers Martigli et al." ;
  bsc:invasiveness "non-invasive" ;
  bsc:evidenceLevel 2.3 ;
  bsc:evidenceCurator "Claude Sonnet 4.5 (2025-11-17)" ;
  bsc:evidenceCorpus "Research on guided breathing with audiovisual cues; 8 studies on heart rate variability (HRV) and coherence training (2017-2024)" ;
  bsc:evidenceReasoning "Moderate evidence for improving breathing coherence and HRV. Audiovisual guidance more effective than audio-only or visual-only cues for maintaining consistent breathing rhythm. Limited large-scale clinical trials." ;
  bsc:contraindications "Generally safe. Rare: anxiety in individuals uncomfortable with paced breathing. Not recommended during acute respiratory distress or hyperventilation episodes." ;
  rdfs:seeAlso <http://purl.bioontology.org/ontology/MESH/D001945> .  # MeSH: Breathing Exercises

bsc:SynestheticStimulation a owl:Class ;
  rdfs:subClassOf bsc:AudiovisualTechnique ;
  rdfs:label "Synesthetic Stimulation" ;
  rdfs:comment "Audiovisual techniques designed to simulate synesthetic experiences by mapping sound frequencies to colors or visual patterns (e.g., low frequencies to red, high frequencies to blue). Used in immersive meditation and consciousness exploration." ;
  bsc:invasiveness "non-invasive" ;
  bsc:evidenceLevel 1.5 ;
  bsc:evidenceCurator "Claude Sonnet 4.5 (2025-11-17)" ;
  bsc:evidenceCorpus "Limited peer-reviewed research; 5 studies on cross-modal perception and immersive experiences; user experience data from VR meditation apps" ;
  bsc:evidenceReasoning "Minimal controlled research. Effects likely mediated through aesthetic engagement and attention focusing rather than direct neurological synesthesia. Popular in VR/AR wellness applications." ;
  bsc:contraindications "SERIOUS: Photosensitive epilepsy if visual component includes flicker. Possible sensory overload, disorientation, or nausea in sensitive individuals." .

bsc:VibroacousticTherapy a owl:Class ;
  rdfs:subClassOf bsc:MultimodalTechnique ;
  rdfs:label "Vibroacoustic Therapy" ;
  rdfs:comment "Combines low-frequency sound (30-120Hz) delivered through speakers and tactile transducers (vibrating surfaces) for whole-body vibrational stimulation. Used in clinical settings for pain management and relaxation." ;
  bsc:invasiveness "minimally-invasive" ;
  bsc:evidenceLevel 2.8 ;
  bsc:evidenceCurator "Claude Sonnet 4.5 (2025-11-17)" ;
  bsc:evidenceCorpus "PubMed search: 'vibroacoustic therapy pain' (2010-2024), 18 clinical studies on pain management, Parkinson's disease, and anxiety reduction" ;
  bsc:evidenceReasoning "Moderate evidence from clinical trials for pain reduction and muscle relaxation. Well-established in Nordic countries (Finland, Norway) as complementary therapy. Meta-analyses show modest but significant effects." ;
  bsc:contraindications "Pregnancy (vibration may affect fetus), deep vein thrombosis (risk of dislodging clots), acute inflammation, recent surgery, pacemakers (low risk but manufacturer precaution). Generally safe for most individuals." ;
  rdfs:seeAlso <https://en.wikipedia.org/wiki/Vibroacoustic_therapy> .

bsc:HapticEntrainment a owl:Class ;
  rdfs:subClassOf bsc:Technique ;
  rdfs:label "Haptic Entrainment" ;
  rdfs:comment "Rhythmic tactile stimulation (vibration, tapping) at specific frequencies to influence brainwave activity or induce relaxation. Can be combined with audio/visual modalities." ;
  bsc:invasiveness "non-invasive" ;
  bsc:evidenceLevel 1.8 ;
  bsc:evidenceCurator "Claude Sonnet 4.5 (2025-11-17)" ;
  bsc:evidenceCorpus "Emerging research area; 6 studies on tactile rhythm perception and cortical entrainment (2019-2024)" ;
  bsc:evidenceReasoning "Early-stage research. Some evidence for tactile rhythm perception and cortical activation. Less studied than audio or visual entrainment. Potential for accessibility applications (hearing/vision impaired individuals)." ;
  bsc:contraindications "Generally safe. Skin sensitivity, peripheral neuropathy may reduce effectiveness or cause discomfort. Avoid over damaged skin or acute injuries." .

bsc:VibrationCue a owl:Class ;
  rdfs:subClassOf bsc:HapticEntrainment ;
  rdfs:label "Vibration Cue" ;
  rdfs:comment "Localized vibrotactile pulses (continuous or pulsed) delivered through wearables, chairs, or handheld devices for grounding, pacing, or tactile entrainment." ;
  bsc:invasiveness "non-invasive" ;
  bsc:evidenceLevel 1.6 ;
  bsc:evidenceCurator "BioSynCare Lab (2025-11-23)" ;
  bsc:evidenceCorpus "Wearable haptics research, ADHD fidget devices with vibration feedback, clinical grounding protocols for trauma therapy." ;
  bsc:evidenceReasoning "Solid user research and early clinical pilots but limited large-sample trials. Effective for grounding and focus support." ;
  bsc:contraindications "Avoid open wounds, acute inflammation, or implanted stimulators unless cleared by manufacturer guidance." ;
  rdfs:seeAlso <http://purl.bioontology.org/ontology/MESH/D014110> ,
               <http://purl.bioontology.org/ontology/MESH/D014732> ;
  skos:related <http://purl.bioontology.org/ontology/MESH/D055698> .

bsc:ImmersiveVRNeurostimulation a owl:Class ;
  rdfs:subClassOf bsc:MultimodalTechnique ;
  rdfs:label "Immersive VR Neurostimulation" ;
  rdfs:comment "Virtual reality environments combining audiovisual entrainment, spatial audio, and interactive elements for immersive neurosensory stimulation. Emerging application of VR for meditation, therapy, and cognitive enhancement." ;
  bsc:invasiveness "non-invasive" ;
  bsc:evidenceLevel 2.2 ;
  bsc:evidenceCurator "Claude Sonnet 4.5 (2025-11-17)" ;
  bsc:evidenceCorpus "VR meditation and therapy research (2018-2024); 11 pilot studies on anxiety, PTSD, and pain management in VR environments" ;
  bsc:evidenceReasoning "Emerging field with promising pilot data. VR immersion enhances engagement and presence compared to 2D audiovisual stimulation. Limited long-term efficacy studies. Rapid growth in commercial VR wellness apps." ;
  bsc:contraindications "SERIOUS: Photosensitive epilepsy (VR flicker and motion). Motion sickness, disorientation, eye strain (common VR side effects). Not recommended for individuals with severe balance disorders or vestibular issues. Avoid prolonged sessions (>30 minutes without breaks)." ;
  rdfs:seeAlso <https://en.wikipedia.org/wiki/Virtual_reality_therapy> .

# ============================================================================
# OUTCOMES TAXONOMY MODULE
# ============================================================================

# Top-level outcome domains

bsc:CognitiveOutcome a owl:Class ;
  rdfs:subClassOf bsc:Outcome ;
  rdfs:label "Cognitive Outcome" ;
  rdfs:comment "Outcomes related to mental processes including attention, memory, executive function, processing speed, and cognitive flexibility." ;
  rdfs:seeAlso <http://purl.bioontology.org/ontology/MESH/D003071> .  # MeSH: Cognition

bsc:EmotionalOutcome a owl:Class ;
  rdfs:subClassOf bsc:Outcome ;
  rdfs:label "Emotional Outcome" ;
  rdfs:comment "Outcomes related to affective states, mood, emotional regulation, and psychological well-being." ;
  rdfs:seeAlso <http://purl.bioontology.org/ontology/MESH/D004644> .  # MeSH: Emotions

bsc:PhysiologicalOutcome a owl:Class ;
  rdfs:subClassOf bsc:Outcome ;
  rdfs:label "Physiological Outcome" ;
  rdfs:comment "Outcomes related to measurable biological processes including heart rate variability, brainwave patterns, cortisol levels, and autonomic nervous system activity." ;
  rdfs:seeAlso <http://purl.bioontology.org/ontology/MESH/D010827> .  # MeSH: Physiology

bsc:BehavioralOutcome a owl:Class ;
  rdfs:subClassOf bsc:Outcome ;
  rdfs:label "Behavioral Outcome" ;
  rdfs:comment "Outcomes related to observable actions and behavioral patterns including sleep quality, activity levels, and habit formation." ;
  rdfs:seeAlso <http://purl.bioontology.org/ontology/MESH/D001519> .  # MeSH: Behavior

# Cognitive outcomes

bsc:AttentionEnhancement a owl:Class ;
  rdfs:subClassOf bsc:CognitiveOutcome ;
  rdfs:label "Attention Enhancement" ;
  rdfs:comment "Improved ability to focus, sustain attention, and resist distraction. Measured via continuous performance tests (CPT), attention span assessments, or subjective self-reports." ;
  skos:altLabel "Focus Improvement", "Concentration Enhancement" ;
  bsc:measurementType "mixed" ;
  bsc:measurementTool "Continuous Performance Test (CPT), self-report attention scales" ;
  bsc:validatedScale true ;
  bsc:typicalLatency 0.0 .

bsc:MemoryEnhancement a owl:Class ;
  rdfs:subClassOf bsc:CognitiveOutcome ;
  rdfs:label "Memory Enhancement" ;
  rdfs:comment "Improved working memory, short-term memory, or long-term memory consolidation. Measured via recall tests, recognition tasks, or memory span assessments." ;
  skos:narrower bsc:WorkingMemoryEnhancement, bsc:LongTermMemoryEnhancement .

bsc:WorkingMemoryEnhancement a owl:Class ;
  rdfs:subClassOf bsc:MemoryEnhancement ;
  rdfs:label "Working Memory Enhancement" ;
  rdfs:comment "Improved capacity to hold and manipulate information in short-term memory. Measured via N-back tasks, digit span tests, or operation span tasks." .

bsc:LongTermMemoryEnhancement a owl:Class ;
  rdfs:subClassOf bsc:MemoryEnhancement ;
  rdfs:label "Long-Term Memory Enhancement" ;
  rdfs:comment "Improved consolidation and retrieval of long-term memories. Measured via delayed recall tests, recognition memory tasks, or autobiographical memory assessments." .

bsc:ExecutiveFunctionEnhancement a owl:Class ;
  rdfs:subClassOf bsc:CognitiveOutcome ;
  rdfs:label "Executive Function Enhancement" ;
  rdfs:comment "Improved planning, decision-making, cognitive flexibility, and inhibitory control. Measured via Stroop test, Wisconsin Card Sorting Test, or Trail Making Test." .

bsc:ProcessingSpeedEnhancement a owl:Class ;
  rdfs:subClassOf bsc:CognitiveOutcome ;
  rdfs:label "Processing Speed Enhancement" ;
  rdfs:comment "Faster information processing and reaction times. Measured via reaction time tasks, symbol coding tests, or processing speed indices." .

bsc:CognitiveFlexibilityEnhancement a owl:Class ;
  rdfs:subClassOf bsc:CognitiveOutcome ;
  rdfs:label "Cognitive Flexibility Enhancement" ;
  rdfs:comment "Improved ability to switch between tasks, adapt to changing rules, and think creatively. Measured via task-switching paradigms or set-shifting tests." .

# Emotional outcomes

bsc:AnxietyReduction a owl:Class ;
  rdfs:subClassOf bsc:EmotionalOutcome ;
  rdfs:label "Anxiety Reduction" ;
  rdfs:comment "Decreased anxiety symptoms, worry, or nervousness. Measured via State-Trait Anxiety Inventory (STAI), Generalized Anxiety Disorder (GAD-7) scale, or heart rate variability (HRV) as physiological proxy." ;
  skos:altLabel "Stress Reduction", "Worry Reduction" ;
  bsc:measurementType "mixed" ;
  bsc:measurementTool "State-Trait Anxiety Inventory (STAI), Heart Rate Variability (HRV)" ;
  bsc:validatedScale true ;
  bsc:typicalLatency 0.0 .

bsc:MoodEnhancement a owl:Class ;
  rdfs:subClassOf bsc:EmotionalOutcome ;
  rdfs:label "Mood Enhancement" ;
  rdfs:comment "Improved overall mood, positive affect, or subjective well-being. Measured via Positive and Negative Affect Schedule (PANAS), Profile of Mood States (POMS), or visual analog mood scales." .

bsc:DepressionReduction a owl:Class ;
  rdfs:subClassOf bsc:EmotionalOutcome ;
  rdfs:label "Depression Reduction" ;
  rdfs:comment "Decreased depressive symptoms, anhedonia, or negative affect. Measured via Beck Depression Inventory (BDI), Patient Health Questionnaire (PHQ-9), or clinical assessment." ;
  skos:related bsc:MoodEnhancement .

bsc:EmotionalRegulationImprovement a owl:Class ;
  rdfs:subClassOf bsc:EmotionalOutcome ;
  rdfs:label "Emotional Regulation Improvement" ;
  rdfs:comment "Enhanced ability to manage, modulate, and respond to emotional experiences adaptively. Measured via Difficulties in Emotion Regulation Scale (DERS) or physiological measures (HRV coherence)." .

bsc:RelaxationResponse a owl:Class ;
  rdfs:subClassOf bsc:EmotionalOutcome ;
  rdfs:label "Relaxation Response" ;
  rdfs:comment "Immediate relaxation state characterized by decreased arousal and subjective calm. Measured via subjective relaxation scales, heart rate, or muscle tension (EMG)." ;
  rdfs:seeAlso <https://en.wikipedia.org/wiki/Relaxation_response> .

# Physiological outcomes

bsc:BrainwaveEntrainment a owl:Class ;
  rdfs:subClassOf bsc:PhysiologicalOutcome ;
  rdfs:label "Brainwave Entrainment" ;
  rdfs:comment "Synchronization of brainwave activity to external stimulus frequency. Measured via EEG spectral analysis showing increased power in target frequency bands (delta, theta, alpha, beta, gamma)." ;
  skos:narrower bsc:AlphaEnhancement, bsc:ThetaEnhancement, bsc:BetaEnhancement, bsc:GammaEnhancement, bsc:DeltaEnhancement .

bsc:AlphaEnhancement a owl:Class ;
  rdfs:subClassOf bsc:BrainwaveEntrainment ;
  rdfs:label "Alpha Enhancement" ;
  rdfs:comment "Increased alpha wave activity (8-12Hz) associated with relaxed alertness and meditative states. Measured via EEG alpha power or alpha/theta ratio." .

bsc:ThetaEnhancement a owl:Class ;
  rdfs:subClassOf bsc:BrainwaveEntrainment ;
  rdfs:label "Theta Enhancement" ;
  rdfs:comment "Increased theta wave activity (4-8Hz) associated with deep meditation, creativity, and light sleep states. Measured via EEG theta power." .

bsc:BetaEnhancement a owl:Class ;
  rdfs:subClassOf bsc:BrainwaveEntrainment ;
  rdfs:label "Beta Enhancement" ;
  rdfs:comment "Increased beta wave activity (12-30Hz) associated with active thinking, focus, and alertness. Measured via EEG beta power." .

bsc:GammaEnhancement a owl:Class ;
  rdfs:subClassOf bsc:BrainwaveEntrainment ;
  rdfs:label "Gamma Enhancement" ;
  rdfs:comment "Increased gamma wave activity (30-100Hz) associated with high-level cognitive processing, perception, and consciousness. Measured via EEG gamma power." .

bsc:DeltaEnhancement a owl:Class ;
  rdfs:subClassOf bsc:BrainwaveEntrainment ;
  rdfs:label "Delta Enhancement" ;
  rdfs:comment "Increased delta wave activity (0.5-4Hz) associated with deep sleep and restorative processes. Measured via EEG delta power." .

bsc:HeartRateVariabilityImprovement a owl:Class ;
  rdfs:subClassOf bsc:PhysiologicalOutcome ;
  rdfs:label "Heart Rate Variability (HRV) Improvement" ;
  rdfs:comment "Increased HRV indicating enhanced autonomic nervous system flexibility and resilience. Measured via RMSSD, SDNN, or coherence ratio from heart rate monitors or ECG." ;
  rdfs:seeAlso <https://en.wikipedia.org/wiki/Heart_rate_variability> .

bsc:CortisolReduction a owl:Class ;
  rdfs:subClassOf bsc:PhysiologicalOutcome ;
  rdfs:label "Cortisol Reduction" ;
  rdfs:comment "Decreased cortisol levels indicating reduced physiological stress response. Measured via salivary cortisol assays." .

bsc:BloodPressureReduction a owl:Class ;
  rdfs:subClassOf bsc:PhysiologicalOutcome ;
  rdfs:label "Blood Pressure Reduction" ;
  rdfs:comment "Decreased systolic and/or diastolic blood pressure. Measured via sphygmomanometer or automated blood pressure monitors." .

bsc:RespiratoryRateReduction a owl:Class ;
  rdfs:subClassOf bsc:PhysiologicalOutcome ;
  rdfs:label "Respiratory Rate Reduction" ;
  rdfs:comment "Slower, deeper breathing patterns associated with relaxation and parasympathetic activation. Measured via respiratory rate monitors or breath counting." .

# Behavioral outcomes

bsc:SleepQualityImprovement a owl:Class ;
  rdfs:subClassOf bsc:BehavioralOutcome ;
  rdfs:label "Sleep Quality Improvement" ;
  rdfs:comment "Enhanced subjective sleep quality, reduced sleep latency, or increased sleep duration. Measured via Pittsburgh Sleep Quality Index (PSQI), sleep diaries, or actigraphy." ;
  skos:narrower bsc:SleepLatencyReduction, bsc:SleepDurationIncrease ;
  bsc:measurementType "mixed" ;
  bsc:measurementTool "Pittsburgh Sleep Quality Index (PSQI), actigraphy" ;
  bsc:validatedScale true ;
  bsc:typicalLatency 1.0 .

bsc:SleepLatencyReduction a owl:Class ;
  rdfs:subClassOf bsc:SleepQualityImprovement ;
  rdfs:label "Sleep Latency Reduction" ;
  rdfs:comment "Faster transition from wakefulness to sleep. Measured via sleep diaries, actigraphy, or polysomnography." .

bsc:SleepDurationIncrease a owl:Class ;
  rdfs:subClassOf bsc:SleepQualityImprovement ;
  rdfs:label "Sleep Duration Increase" ;
  rdfs:comment "Longer total sleep time. Measured via sleep diaries, actigraphy, or polysomnography." .

bsc:PhysicalActivityIncrease a owl:Class ;
  rdfs:subClassOf bsc:BehavioralOutcome ;
  rdfs:label "Physical Activity Increase" ;
  rdfs:comment "Increased physical activity levels, movement, or exercise frequency. Measured via activity trackers, pedometers, or self-report questionnaires." .

bsc:PainReduction a owl:Class ;
  rdfs:subClassOf bsc:BehavioralOutcome ;
  rdfs:label "Pain Reduction" ;
  rdfs:comment "Decreased subjective pain intensity or pain-related interference. Measured via Visual Analog Scale (VAS), Numeric Rating Scale (NRS), or McGill Pain Questionnaire." ;
  rdfs:seeAlso <http://purl.bioontology.org/ontology/MESH/D010146> .  # MeSH: Pain
