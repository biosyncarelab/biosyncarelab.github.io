[ rdf:type owl:Ontology ;
    dct:title "SSO Ontology (extended mirror)"@en ;
    dct:created "2025-11-05"^^xsd:date ;
    owl:versionInfo "snapshot"
] .

#################################################################
#    Annotation properties
#################################################################

###  http://www.w3.org/2004/02/skos/core#definition
skos:definition rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#prefLabel
skos:prefLabel rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#related
skos:related rdf:type owl:AnnotationProperty .


###  https://acca-harmonicare-chromosound.netlify.app/rdf/SSO_Ontology.owl#ActiveCollaborator
sso:ActiveCollaborator rdf:type owl:AnnotationProperty .


###  https://acca-harmonicare-chromosound.netlify.app/rdf/SSO_Ontology.owl#ConfirmedCollaborator
sso:ConfirmedCollaborator rdf:type owl:AnnotationProperty .


###  https://acca-harmonicare-chromosound.netlify.app/rdf/SSO_Ontology.owl#PotentialCollaborator
sso:PotentialCollaborator rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  https://acca-harmonicare-chromosound.netlify.app/rdf/SSO_Ontology.owl#administeredBy
sso:administeredBy rdf:type owl:ObjectProperty ;
                   rdfs:domain sso:Session ;
                   rdfs:range sso:Expert ;
                   rdfs:comment "Session is administered by a specialist or organization." .


###  https://acca-harmonicare-chromosound.netlify.app/rdf/SSO_Ontology.owl#appliesTechnique
sso:appliesTechnique rdf:type owl:ObjectProperty ;
                     rdfs:domain sso:Expert ;
                     rdfs:range sso:Technique ;
                     rdfs:comment "Technique applied by an expert." .


###  https://acca-harmonicare-chromosound.netlify.app/rdf/SSO_Ontology.owl#employsTechnique
sso:employsTechnique rdf:type owl:ObjectProperty ;
                     rdfs:domain sso:Neuromodulation ;
                     rdfs:range sso:Technique ;
                     rdfs:comment "Neuromodulation employs specific techniques." .


###  https://acca-harmonicare-chromosound.netlify.app/rdf/SSO_Ontology.owl#enhances
sso:enhances rdf:type owl:ObjectProperty ;
             rdfs:domain sso:Technique ;
             rdfs:range sso:CognitiveModulation ;
             rdfs:comment "Technique that enhances cognitive functions." .


###  https://acca-harmonicare-chromosound.netlify.app/rdf/SSO_Ontology.owl#isPotentialCollaborator
sso:isPotentialCollaborator rdf:type owl:ObjectProperty ;
                            rdfs:domain sso:Expert ;
                            rdfs:range sso:Collaborator ;
                            rdfs:comment "Potential collaboration between experts and SSO." .


###  https://acca-harmonicare-chromosound.netlify.app/rdf/SSO_Ontology.owl#providedBy
sso:providedBy rdf:type owl:ObjectProperty ;
               rdfs:domain sso:SpecializedNeuromodulationDevice ;
               rdfs:range sso:Company ;
               rdfs:comment "Device provided by a company for neuromodulation." .


###  https://acca-harmonicare-chromosound.netlify.app/rdf/SSO_Ontology.owl#recommendsTechnique
sso:recommendsTechnique rdf:type owl:ObjectProperty ;
                        rdfs:domain sso:Collaborator ;
                        rdfs:range sso:Technique ;
                        rdfs:comment "Technique recommended by a collaborator." .


###  https://acca-harmonicare-chromosound.netlify.app/rdf/SSO_Ontology.owl#supportsDevice
sso:supportsDevice rdf:type owl:ObjectProperty ;
                   rdfs:domain sso:Company ;
                   rdfs:range sso:SpecializedNeuromodulationDevice ;
                   rdfs:comment "Device supported by the company." .


###  https://acca-harmonicare-chromosound.netlify.app/rdf/SSO_Ontology.owl#usesDevice
sso:usesDevice rdf:type owl:ObjectProperty ;
               rdfs:domain sso:Session ;
               rdfs:range sso:Device ;
               rdfs:comment "Session uses a specific device for stimulation." .


#################################################################
#    Classes
#################################################################

###  http://www.w3.org/2004/02/skos/core#Concept
skos:Concept rdf:type owl:Class .


###  https://acca-harmonicare-chromosound.netlify.app/rdf/SSO_Ontology.owl#AudioStimulation
sso:AudioStimulation rdf:type owl:Class ;
                     rdfs:subClassOf sso:AudiovisualStimulation ;
                     rdfs:comment "Stimulation using audio signals like isochronic or binaural beats." ;
                     rdfs:label "AudioStimulation" .


###  https://acca-harmonicare-chromosound.netlify.app/rdf/SSO_Ontology.owl#AudioTechniques
sso:AudioTechniques rdf:type owl:Class ;
                    rdfs:subClassOf sso:Technique ;
                    rdfs:comment "Audio-based modulation techniques for brain and sensory stimulation." ;
                    rdfs:label "AudioTechniques" .


###  https://acca-harmonicare-chromosound.netlify.app/rdf/SSO_Ontology.owl#AudiovisualStimulation
sso:AudiovisualStimulation rdf:type owl:Class ;
                           rdfs:subClassOf sso:SensoryStimulation ;
                           rdfs:comment "Stimulation using both audio and visual cues together." ;
                           rdfs:label "AudiovisualStimulation" .


###  https://acca-harmonicare-chromosound.netlify.app/rdf/SSO_Ontology.owl#BinauralBeats
sso:BinauralBeats rdf:type owl:Class ;
                  rdfs:subClassOf sso:AudioTechniques ;
                  rdfs:comment "Dual-tone auditory beats used to create a perceived third beat for entrainment." ;
                  rdfs:label "BinauralBeats" .


###  https://acca-harmonicare-chromosound.netlify.app/rdf/SSO_Ontology.owl#BrainStimulation
sso:BrainStimulation rdf:type owl:Class ;
                     rdfs:subClassOf sso:Neuromodulation ;
                     rdfs:comment "Direct stimulation targeted at brain regions for therapeutic effects." ;
                     rdfs:label "BrainStimulation" .


###  https://acca-harmonicare-chromosound.netlify.app/rdf/SSO_Ontology.owl#BrainwaveEntrainment
sso:BrainwaveEntrainment rdf:type owl:Class ;
                         rdfs:subClassOf sso:Technique ;
                         rdfs:comment "Techniques aligning brainwave frequencies with external rhythms." ;
                         rdfs:label "BrainwaveEntrainment" .


###  https://acca-harmonicare-chromosound.netlify.app/rdf/SSO_Ontology.owl#ChemicalStimulation
sso:ChemicalStimulation rdf:type owl:Class ;
                        rdfs:subClassOf sso:Neuromodulation ;
                        rdfs:comment "Stimulation using chemical agents or drugs." ;
                        rdfs:label "ChemicalStimulation" .


###  https://acca-harmonicare-chromosound.netlify.app/rdf/SSO_Ontology.owl#Chromosound
sso:Chromosound rdf:type owl:Class ;
                rdfs:subClassOf sso:AudiovisualStimulation ;
                rdfs:comment "A specific type of audiovisual stimulation involving coordinated sound and color." ;
                rdfs:label "Chromosound" .


###  https://acca-harmonicare-chromosound.netlify.app/rdf/SSO_Ontology.owl#CognitiveModulation
sso:CognitiveModulation rdf:type owl:Class ;
                        rdfs:subClassOf sso:Neuromodulation ;
                        rdfs:comment "Modulation techniques that affect cognitive functions and states." ;
                        rdfs:label "CognitiveModulation" .


###  https://acca-harmonicare-chromosound.netlify.app/rdf/SSO_Ontology.owl#Collaborator
sso:Collaborator rdf:type owl:Class ;
                 rdfs:comment "Potential collaborators for the SSO project, including experts and organizations." ;
                 rdfs:label "Collaborator" .


###  https://acca-harmonicare-chromosound.netlify.app/rdf/SSO_Ontology.owl#ColorTherapy
sso:ColorTherapy rdf:type owl:Class ;
                 rdfs:subClassOf sso:VisualTechniques ;
                 rdfs:comment "Therapy using color exposure to influence mental and physical health." ;
                 rdfs:label "ColorTherapy" .


###  https://acca-harmonicare-chromosound.netlify.app/rdf/SSO_Ontology.owl#Company
sso:Company rdf:type owl:Class ;
            rdfs:subClassOf sso:Expert ;
            rdfs:comment "An organization providing neuromodulation devices or services." ;
            rdfs:label "Company" .


###  https://acca-harmonicare-chromosound.netlify.app/rdf/SSO_Ontology.owl#Context
sso:Context rdf:type owl:Class ;
            rdfs:subClassOf sso:CognitiveModulation ;
            rdfs:comment "Context in which stimulation is administered, impacting efficacy." ;
            rdfs:label "Context" .


###  https://acca-harmonicare-chromosound.netlify.app/rdf/SSO_Ontology.owl#DeepBrainStimulation
sso:DeepBrainStimulation rdf:type owl:Class ;
                         rdfs:subClassOf sso:ElectricalStimulation ;
                         rdfs:comment "Stimulation of deep brain structures using implanted electrodes." ;
                         rdfs:label "DeepBrainStimulation" .


###  https://acca-harmonicare-chromosound.netlify.app/rdf/SSO_Ontology.owl#Device
sso:Device rdf:type owl:Class ;
           rdfs:comment "Device used for various stimulation techniques." ;
           rdfs:label "Device" .


###  https://acca-harmonicare-chromosound.netlify.app/rdf/SSO_Ontology.owl#ElectricalStimulation
sso:ElectricalStimulation rdf:type owl:Class ;
                          rdfs:subClassOf sso:BrainStimulation ;
                          rdfs:comment "Stimulation via low-level electrical currents." ;
                          rdfs:label "ElectricalStimulation" .


###  https://acca-harmonicare-chromosound.netlify.app/rdf/SSO_Ontology.owl#Expert
sso:Expert rdf:type owl:Class ;
           rdfs:comment "An expert in neuromodulation, could be a person or a company." ;
           rdfs:label "Expert" .


###  https://acca-harmonicare-chromosound.netlify.app/rdf/SSO_Ontology.owl#Flickering
sso:Flickering rdf:type owl:Class ;
               rdfs:subClassOf sso:VisualTechniques ;
               rdfs:comment "Visual technique using flickering lights to stimulate the brain." ;
               rdfs:label "Flickering" .


###  https://acca-harmonicare-chromosound.netlify.app/rdf/SSO_Ontology.owl#GeneralPurposeDevice
sso:GeneralPurposeDevice rdf:type owl:Class ;
                         rdfs:subClassOf sso:Device ;
                         rdfs:comment "Devices like smartphones and laptops that can run software for stimulation." ;
                         rdfs:label "GeneralPurposeDevice" .


###  https://acca-harmonicare-chromosound.netlify.app/rdf/SSO_Ontology.owl#GroupSession
sso:GroupSession rdf:type owl:Class ;
                 rdfs:subClassOf sso:Session ;
                 rdfs:comment "A session involving multiple users undergoing the same stimulation protocol." ;
                 rdfs:label "GroupSession" .


###  https://acca-harmonicare-chromosound.netlify.app/rdf/SSO_Ontology.owl#GuidingStimulation
sso:GuidingStimulation rdf:type owl:Class ;
                       rdfs:subClassOf sso:SensoryStimulation ;
                       rdfs:comment "Stimulation guided by rhythms or cycles, such as breathing cycles." ;
                       rdfs:label "GuidingStimulation" .


###  https://acca-harmonicare-chromosound.netlify.app/rdf/SSO_Ontology.owl#IndividualSession
sso:IndividualSession rdf:type owl:Class ;
