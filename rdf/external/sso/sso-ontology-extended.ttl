                      rdfs:comment "Sounds with equal intervals, often used for entrainment." ;
                      rdfs:label "IsochronousSounds" .


###  https://acca-harmonicare-chromosound.netlify.app/rdf/SSO_Ontology.owl#Language
sso:Language rdf:type owl:Class ;
             rdfs:subClassOf sso:CognitiveModulation ;
             rdfs:comment "Stimulation involving human language." ;
             rdfs:label "Language" .


###  https://acca-harmonicare-chromosound.netlify.app/rdf/SSO_Ontology.owl#MonauralBeats
sso:MonauralBeats rdf:type owl:Class ;
                  rdfs:subClassOf sso:AudioTechniques ;
                  rdfs:comment "Single-tone auditory beats used for stimulation." ;
                  rdfs:label "MonauralBeats" .


###  https://acca-harmonicare-chromosound.netlify.app/rdf/SSO_Ontology.owl#Neuromodulation
sso:Neuromodulation rdf:type owl:Class ;
                    rdfs:comment "Techniques to alter brain activity through various forms of stimulation." ;
                    rdfs:label "Neuromodulation" .


###  https://acca-harmonicare-chromosound.netlify.app/rdf/SSO_Ontology.owl#SensoryStimulation
sso:SensoryStimulation rdf:type owl:Class ;
                       rdfs:subClassOf sso:Neuromodulation ;
                       rdfs:comment "Stimulation via sensory inputs such as audio, visual, and tactile cues." ;
                       rdfs:label "SensoryStimulation" .


###  https://acca-harmonicare-chromosound.netlify.app/rdf/SSO_Ontology.owl#Session
sso:Session rdf:type owl:Class ;
            rdfs:comment "A session instance involving stimulation, either guided or unguided." ;
            rdfs:label "Session" .


###  https://acca-harmonicare-chromosound.netlify.app/rdf/SSO_Ontology.owl#SpecialistPerson
sso:SpecialistPerson rdf:type owl:Class ;
                     rdfs:subClassOf sso:Expert ;
                     rdfs:comment "A human specialist in neuromodulation techniques." ;
                     rdfs:label "SpecialistPerson" .


###  https://acca-harmonicare-chromosound.netlify.app/rdf/SSO_Ontology.owl#SpecializedNeuromodulationDevice
sso:SpecializedNeuromodulationDevice rdf:type owl:Class ;
                                     rdfs:subClassOf sso:Device ;
                                     rdfs:comment "Devices specifically built for neuromodulation, like MindAlive devices." ;
                                     rdfs:label "SpecializedNeuromodulationDevice" .


###  https://acca-harmonicare-chromosound.netlify.app/rdf/SSO_Ontology.owl#TactileStimulation
sso:TactileStimulation rdf:type owl:Class ;
                       rdfs:subClassOf sso:SensoryStimulation ;
                       rdfs:comment "Stimulation through tactile feedback, using vibrations or touch." ;
                       rdfs:label "TactileStimulation" .


###  https://acca-harmonicare-chromosound.netlify.app/rdf/SSO_Ontology.owl#Technique
sso:Technique rdf:type owl:Class ;
              rdfs:comment "Techniques used for neuromodulation." ;
              rdfs:label "Technique" .


###  https://acca-harmonicare-chromosound.netlify.app/rdf/SSO_Ontology.owl#TranscranialMagneticStimulation
sso:TranscranialMagneticStimulation rdf:type owl:Class ;
                                    rdfs:subClassOf sso:TranscranialStimulation ;
                                    rdfs:comment "Magnetic stimulation applied to the brain non-invasively." ;
                                    rdfs:label "TranscranialMagneticStimulation" .


###  https://acca-harmonicare-chromosound.netlify.app/rdf/SSO_Ontology.owl#TranscranialStimulation
sso:TranscranialStimulation rdf:type owl:Class ;
                            rdfs:subClassOf sso:Neuromodulation ;
                            rdfs:comment "Techniques applied directly to the skull or scalp." ;
                            rdfs:label "TranscranialStimulation" .


###  https://acca-harmonicare-chromosound.netlify.app/rdf/SSO_Ontology.owl#VisualStimulation
sso:VisualStimulation rdf:type owl:Class ;
                      rdfs:subClassOf sso:AudiovisualStimulation ;
                      rdfs:comment "Stimulation using visual signals, including flickering lights and color." ;
                      rdfs:label "VisualStimulation" .


###  https://acca-harmonicare-chromosound.netlify.app/rdf/SSO_Ontology.owl#VisualTechniques
sso:VisualTechniques rdf:type owl:Class ;
                     rdfs:subClassOf sso:Technique ;
                     rdfs:comment "Visual-based modulation techniques for stimulation." ;
                     rdfs:label "VisualTechniques" .


